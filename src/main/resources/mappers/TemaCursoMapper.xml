<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="Mappers.TemaCursoMapper">
    <resultMap type='TemaCurso' id='TemaCurso'>
        <id property='NombreDelTema' column='nombreDelTema'/>
        <result property='Descripcion' column = 'descripcion'/>
        <association property='curso' javaType='Curso' resultMap='Mappers.CursoMapper.Curso'></association>
        <collection property='asesorias' ofType='Asesoria' resultMap='Mappers.AsesoriaMapper.Asesoria' ></collection>
    </resultMap>
</mapper>